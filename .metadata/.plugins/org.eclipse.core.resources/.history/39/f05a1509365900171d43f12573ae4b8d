package Lab2;

public class DListTester {

	public static void main(String[] args) {
		
		//generate empty list
		DList<String> testList= new DList<String>();
		
		String test = "test";
	
		int i = 0;
		while (i <=  11) {
			testList.addNode(i, (test+ ":  " +i));
			i++;
		}
		
		testList.toString();

		System.out.println(testList.getItem(5));
		System.out.println(testList.getFirst());
		System.out.println(testList.getLast());
		
		System.out.println();
		System.out.println();

		testList.toString();

	
		
		
	}

}
