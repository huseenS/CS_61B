package Lab2;

public class ListTester {

	public static void main(String[] args) {
		
		//generate empty list
		DList<String> testList= new DList<String>();
		
		String first = new String();
		
		for (int i = 0; i < 11;  i++) {
			testList.addNode(i, first + ":  " + i);
		}
		
		System.out.println(testList.getItem(5));
		System.out.println(testList.getFirst());
		System.out.println(testList.getLast());
		
		System.out.println();
		System.out.println();

		testList.toString();

	
		
		
	}

}
