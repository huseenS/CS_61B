package Homework2;

public class ListTester {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String test ="test";
		
		//generate empty list
		SList<String> test1 = new SList<String>(test);
		
		//add a few items
		test1.insertFront("inserted at front");
		test1.insertBack("inserted at Back");
		test1.insertFront("another front insert");
		
		//check size of list
		test1.size();
		
		//delete 2nd node
		test1.deleteNodeAt(1);
		
		test1.size();
	}

}
