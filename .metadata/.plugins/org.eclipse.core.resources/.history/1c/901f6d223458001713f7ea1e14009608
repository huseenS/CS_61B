package Lab2;

/**
 * @author Huseen Sufi
 *Doubly Linked List implementation, accepts generic object
 */
public class DList <T>{
	private DListNode next;
	private DListNode prev;
	
 //Nested inner DListNode class
	public class DListNode {
		private DListNode head;
		private DListNode tail;
		private T item; // generic Object
		
		//constructor 
		public DListNode(DListNode head, DListNode tail, T item) {
			this.head = head;
			this.tail = tail;
			this.item = item;
		}
		//getters
		public DListNode getHead() {
			return head;
		}
		
		public T getItem() {
			return item;
		}
		public DListNode getTail() {
			return tail;
		}
		
		//setters
		public void setHead(DListNode head) {
			this.head = head;
		}
		public void setTail(DListNode tail) {
			this.tail = tail;
		}
		public void setItem(T item) {
			this.item = item;
		}
	}
	//DList Methods: Insert, Delete, Sizeof, IsEmpty
	
	//DListConstructor
	
	
	
}
